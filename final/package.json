{
  "name": "angular1-scaffold",
  "version": "1.0.0",
  "description": "Angular 1 scaffold with Typescript and ng-metadata",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngParty/Angular1-scaffold.git"
  },
  "scripts": {
    "start": "npm run server",
    "build": "NODE_ENV=production webpack --config webpack-build.config.js",
    "build:dev": "webpack --progress --profile --colors",
    "test": "karma start",
    "test:e2e": "protractor protractor.conf.js",
    "test:ci": "karma start --browsers Firefox",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "clean": "rimraf doc/ coverage/ dist/",
    "server": "webpack-dev-server --inline --progress --profile --colors --watch --content-base src/"
  },
  "keywords": [],
  "contributors": [
    "Martin Hochel <hochelmartin@gmail.com>",
    "Mario Vejlupek <mario@vejlupek.cz>"
  ],
  "license": "ISC",
  "dependencies": {
    "angular": "1.7.4",
    "core-js": "2.5.7",
    "ng-metadata": "4.0.1",
    "rxjs": "6.3.2",
    "tslib": "1.9.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/compiler": "6.1.7",
    "@angular/core": "6.1.7",
    "@types/angular": "1.6.51",
    "@types/angular-mocks": "1.7.0",
    "@types/core-js": "2.5.0",
    "@types/jasmine": "2.8.8",
    "@types/lodash": "4.14.116",
    "@types/node": "10.10.1",
    "@types/protractor": "^4.0.0",
    "angular-mocks": "1.7.4",
    "awesome-typescript-loader": "5.2.1",
    "codelyzer": "4.4.4",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "html-webpack-plugin": "3.2.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine-core": "3.2.1",
    "json-loader": "0.5.7",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "1.1.2",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.5",
    "lodash": "4.17.11",
    "node-sass": "4.9.3",
    "protractor": "^5.4.1",
    "protractor-screenshoter-plugin": "^0.10.1",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.12.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.0",
    "ts-node": "^7.0.1",
    "tslint": "5.11.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.0.3",
    "webpack": "4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "3.1.8"
  },
  "engines": {
    "node": ">= 6.x",
    "npm": ">= 3"
  }
}
